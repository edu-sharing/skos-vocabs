@base <https://w3id.org/edu-sharing/vocabs/lel/taxon/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<scheme> a skos:ConceptScheme ;
    dct:title "Landwirtschaft – Themenstruktur"@de ;
    dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept <landwirtschaft> .

<landwirtschaft> a skos:Concept ;
    skos:prefLabel "Landwirtschaft"@de ;
    skos:topConceptOf <scheme> ;
    skos:narrower <tierische-erzeugung>, <pflanzliche-erzeugung>, <betriebswirtschaft-unternehmensfuehrung> .

### --- TIERISCHE ERZEUGUNG ---
<tierische-erzeugung> a skos:Concept ;
    skos:prefLabel "Tierische Erzeugung"@de ;
    skos:broader <landwirtschaft> ;
    skos:narrower <rinderhaltung>, <schweinehaltung>, <andere-tierhaltungsverfahren> .

